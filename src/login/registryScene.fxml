<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="login.RegistrySceneController">
   <children>
      <Label fx:id="nameLabel" layoutX="90.0" layoutY="112.0" text="Nombre">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="90.0" layoutY="179.0" text="Contraseña">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </Label>
      <Label fx:id="validatePasswordLabel" layoutX="90.0" layoutY="238.0" text="Validar Contraseña">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="nametf" layoutX="300.0" layoutY="107.0" />
      <TextField fx:id="passwordtf" layoutX="300.0" layoutY="174.0" />
      <TextField fx:id="validatePasswordtf" layoutX="300.0" layoutY="233.0" />
      <Label fx:id="registryLabel" layoutX="253.0" layoutY="37.0" text="Registro">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="resgistryBtn" layoutX="387.0" layoutY="318.0" mnemonicParsing="false" onAction="#registryBtnAction" text="Registrarme" />
   </children>
</AnchorPane>
